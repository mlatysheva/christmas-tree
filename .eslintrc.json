{
  "plugins": ["prettier", "import"],
  "extends": [
      "airbnb-base",
      "airbnb-typescript/base",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/recommended-requiring-type-checking",
      "prettier"      
  ],
  "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
  },
  "env": {
      "es6": true,
      "browser": true,
      "node": true
  },
  "rules": {
      "no-debugger": "off",
      "no-console": 0,
      "class-methods-use-this": "off"
  }
}